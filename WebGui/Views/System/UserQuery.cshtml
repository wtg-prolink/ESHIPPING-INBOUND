        $("#ItsdArDailog").on("show.bs.modal", function () {
            var now = new Date();
            var year = now.getFullYear();
            var month = (now.getMonth() + 1).toString().padStart(2, '0');
            var day = now.getDate().toString().padStart(2, '0');
            $("#Model").val("");
            $("#Itsd").val("");
            $("#FunctionClass").val("");
            $("#ItsdStatus").val("");
            $("#RequestDateFrom").val(year + "/" + month + "/" + day + " 00:00:00");
