<script type="text/javascript">
    function BackApprove() {
        var cancelreson = $("#BackRemark").val();
        if (cancelreson == "") {
            CommonFunc.Notify("", "@Resources.Locale.L_ActManage_EnterReason", 500, "warning");
            return;
        }

        var shipments = $("#ShipmentId").val();
        ajaxHttp(rootPath + "BookingAction/ChangeShipDate", { "shipmentid": shipments, "cancelreson": cancelreson, autoReturnData: false },
       function (result) {
           if (result.IsOk == "Y") {
               CommonFunc.Notify("", result.message, 500, "success");
               $("#CloseBookingWin").trigger("click");
           }
           else {
               CommonFunc.Notify("", result.message, 500, "warning");
               $("#CloseBookingWin").trigger("click");
           }
           MenuBarFuncArr.MBCancel();
           return true;
       });
    }
</script>
