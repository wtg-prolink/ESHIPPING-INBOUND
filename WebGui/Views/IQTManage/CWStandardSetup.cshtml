@{
    ViewBag.Title = "CWStandardSetup";
}
<script type="text/javascript">
	var _uid = "@ViewBag.Uid";
	var schemas = "@ViewBag.schemas.Content";
</script>
<script type="text/javascript" src="@Url.Content("~/Scripts/TKBL/initView.js")"></script>
<script type="text/javascript" src="@Url.Content("~/Scripts/IQTManage/CWStandardSetup.js?20231210")"></script>
<div id="wrapper">
    <div id="page-wrapper">
        <div class="container-fluid">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="pure-g">
                        <div class="pure-u-sm-5-60">
                            <label for="LspCd" class="control-label">@Resources.Locale.L_AirSetup_LspCd</label>
                        </div>
                        <div class="pure-u-sm-10-60 control-group">
                            <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="LspCd" name="LspCd" fieldname="LspCd" />
                                <span class="input-group-btn">
                                    <button class="btn btn-sm btn-default" type="button" id="LspCdLookup">
                                        <span class="glyphicon glyphicon-search"></span>
                                    </button>
                                </span>
                            </div>
                        </div>
                        <div class="pure-u-sm-15-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="LspNm" fieldname="LspNm" name="LspNm" readonly />
                        </div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="TranType" class="control-label">@Resources.Locale.L_DNApproveManage_TranMode</label>
                        </div>
                        <div class="pure-u-sm-10-60">
                            <select class="form-control input-sm" dt="mt" id="TranType" name="TranType" fieldname="TranType">
                                <option value=""></option>
                                <option value="D">D:INLAND EXPRESS</option>
                                <option value="F">F:FCL</option>
                                <option value="L">L:LCL</option>
                                <option value="A">A:AIR</option>
                                <option value="E">E:INT’L EXPRESS</option>
                                <option value="T">T:Truck</option>
                                <option value="R">R:Railway</option>
                            </select>
                        </div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="Bw" class="control-label">@Resources.Locale.L_CWStandard_CW</label>
                        </div>
                        <div class="pure-u-sm-10-60 control-group">
                            <input type="text" class="form-control input-sm" dt="mt" id="Bw" name="Bw" fieldname="Bw" />
                            <p class="help-block tooltips"></p>
                        </div>
                    </div>
                    <div class="pure-g">
                        <div class="pure-u-sm-5-60">
                            <label for="Pcw" class="control-label">@Resources.Locale.L_PCWStandard_CW</label>
                        </div>
                        <div class="pure-u-sm-10-60 control-group">
                            <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="Pcw" name="Pcw" fieldname="Pcw" />
                                <p class="help-block tooltips"></p>
                            </div>
                        </div>

                    </div>
                    <div class="pure-g">
                        <div class="pure-u-sm-5-60">
                            <label for="CreateCmp" class="control-label">@Resources.Locale.L_IQTManage_CreateBy</label>
                        </div>
                        <div class="pure-u-sm-7-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="CreateBy" name="CreateBy" fieldname="CreateBy" readonly />
                        </div>
                        <div class="pure-u-sm-10-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="CreateDate" name="CreateDate" fieldname="CreateDate" readonly>
                        </div>
                        <div class="pure-u-sm-13-60 label-right">
                            <label for="ModifyBy" class="control-label">@Resources.Locale.L_IQTManage_ModifyDate</label>
                        </div>
                        <div class="pure-u-sm-7-60 control-group">
                            <input type="text" class="form-control input-sm" dt="mt" id="ModifyBy" name="ModifyBy" fieldname="ModifyBy" readonly />
                        </div>
                        <div class="pure-u-sm-10-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="ModifyDate" name="ModifyDate" fieldname="ModifyDate" readonly />
                        </div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="Cmp" class="control-label">Location</label>
                        </div>
                        <div class="pure-u-sm-3-60 control-group">
                            <input type="text" class="form-control input-sm" dt="mt" id="Cmp" name="Cmp" fieldname="Cmp" readonly />
                        </div>
                    </div>

                </div>
             </div>
        </div>
    </div>
</div>
<input type="hidden" dt="mt" id="UId" name="UId" fieldname="UId" isKey="true" />

