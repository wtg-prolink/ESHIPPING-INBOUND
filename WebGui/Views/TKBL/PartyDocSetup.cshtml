                                        <button class="btn btn-sm btn-default" type="button" id="StsCdLookup">
                                            <span class="glyphicon glyphicon-search"></span>
                                        </button>
                                    </span>
                                </div>
                                <p class="help-block tooltips"></p>
                            </div>
                            <div class="pure-u-sm-15-60">
                                <input type="text" class="form-control input-sm" dt="mt" id="StsDescp" name="StsDescp" fieldname="StsDescp" readonly />
                            </div>
                    </div>
                    <div class="pure-g">
                    	<div class="pure-u-sm-5-60">
                    		<label for="Remark" class="control-label">@Resources.Locale.L_PartyDocSetup_Remark</label>
                    	</div>
                    	<div class="pure-u-sm-55-60 control-group">
                    		<textarea class="form-control lastInput" dt="mt" id="Remark" name="Remark" fieldname="Remark"></textarea>
                    	    <p class="help-block tooltips"></p>
                    	</div>
                    </div>

                    <div class="pure-g">
                        <div class="pure-u-sm-5-60">
                            <label for="CreateBy" class="control-label">@Resources.Locale.L_PartyDocSetup_CreateBy</label>
                        </div>
                        <div class="pure-u-sm-7-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="CreateBy" name="CreateBy" fieldname="CreateBy" readonly />
                        </div>
                        <div class="pure-u-sm-7-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="CreateDate" name="CreateDate" fieldname="CreateDate" readonly />
                        </div>
                         <div class="pure-u-sm-21-60"></div>
                        <div class="pure-u-sm-6-60 label-right">
                            <label for="ModifyBy" class="control-label">@Resources.Locale.L_PartyDocSetup_ModifyBy</label>
                        </div>
                        <div class="pure-u-sm-7-60 control-group">
                            <input type="text" class="form-control input-sm" dt="mt" id="ModifyBy" name="ModifyBy" fieldname="ModifyBy" readonly />
                        </div>
                        <div class="pure-u-sm-7-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="ModifyDate" name="ModifyDate" fieldname="ModifyDate" readonly />
