                        <div class="pure-u-sm-10-60 control-group">
                            <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="PartyNo" fieldname="PartyNo" name="PartyNo" />
                                <span class="input-group-btn">
                                    <button class="btn btn-sm btn-default" type="button" id="PartyNoLookup">
                                        <span class="glyphicon glyphicon-search"></span>
                                    </button>
                                </span>
                            </div>
                            <p class="help-block tooltips"></p>
                        </div>
                        <div class="pure-u-sm-15-60">
                            <input type="text" class="form-control input-sm" dt="mt" id="PartyNm" name="PartyNm" fieldname="PartyNm" readonly />
                        </div>
                    </div>
                    <div class="pure-g">
                            <div class="pure-u-sm-5-60">
                                <label for="StsCd" class="control-label" style="color: rgb(255, 0, 0);">@Resources.Locale.L_PartyDocSetup_StsCd</label>
