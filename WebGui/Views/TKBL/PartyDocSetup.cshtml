@{
    ViewBag.Title = "PartyDocSetup";
}

<script type="text/javascript">
    var _uid = "@ViewBag.Uid";
    var _cmp = "@ViewBag.Cmp";
    var schemas = "@ViewBag.schemas.Content";
    var select_tranmode = "@ViewBag.SelectTranMode";
    var default_tranmode = "@ViewBag.DefaultTranMode";
    var select_term = "@ViewBag.SelectTerm";
    var default_term = "@ViewBag.DefaultTerm";
</script>

<div id="wrapper">
    <div id="page-wrapper">
        <input type="hidden" id="UId" dt="mt" fieldname="UId" iskey="true" />
        <div class="container-fluid">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div class="pure-g">
                    	<div class="pure-u-sm-5-60">
                    	    <label for="Cmp" class="control-label" style="color: rgb(255, 0, 0);">@Resources.Locale.L_PartyDocSetup_Cmp</label>
                    	</div>
                    	<div class="pure-u-sm-10-60 control-group">
                    	    <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="Cmp" fieldname="Cmp" name="Cmp" iskey="true" required />
                    	        <span class="input-group-btn">
                    	            <button class="btn btn-sm btn-default" type="button" id="CmpLookup">
                    	                <span class="glyphicon glyphicon-search"></span>
                    	            </button>
                    	        </span>
                    	    </div>
                    	    <p class="help-block tooltips"></p>
                    	</div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="TranMode" class="control-label required">@Resources.Locale.L_PartyDocSetup_TranMode</label>
                        </div>
                        <div class="pure-u-sm-10-60">
                            <select class="form-control input-sm" dt="mt" id="TranMode" name="TranMode" fieldname="TranMode" iskey="true" required></select>
                        </div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="Term" class="control-label">@Resources.Locale.L_PartyDocSetup_Term</label>
                        </div>
                        <div class="pure-u-sm-10-60 control-group">
                            <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="Term" name="Term" fieldname="Term" />
                                <span class="input-group-btn">
                                    <button class="btn btn-sm btn-default" type="button" id="TermLookup">
                                        <span class="glyphicon glyphicon-search"></span>
                                    </button>
                                </span>
                            </div>
                            <p class="help-block tooltips"></p>
                        </div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="FreightTerm" class="control-label">@Resources.Locale.L_PartyDocSetup_FreightTerm</label>
                        </div>
                        <div class="pure-u-sm-10-60">
                            <select class="form-control input-sm" dt="mt" id="FreightTerm" name="FreightTerm" fieldname="FreightTerm">
                                <option value=""></option>
                                <option value="P">P:Prepaid</option>
                                <option value="C">C:Collect</option>
                                <option value="O">O:Other</option>
                            </select>
                        </div>
                    </div>

                    <div class="pure-g">
                    	<div class="pure-u-sm-5-60">
                    	    <label for="PartyType" class="control-label" style="color: rgb(255, 0, 0);">@Resources.Locale.L_PartyDocSetup_PartyType</label>
                    	</div>
                    	<div class="pure-u-sm-10-60 control-group">
                    	    <div class="input-group">
                                <input type="text" class="form-control input-sm" dt="mt" id="PartyType" name="PartyType" fieldname="PartyType" iskey="true" required />
                    	        <span class="input-group-btn">
                    	            <button class="btn btn-sm btn-default" type="button" id="PartyTypeLookup">
                    	                <span class="glyphicon glyphicon-search"></span>
                    	            </button>
                    	        </span>
                    	    </div>
                    	    <p class="help-block tooltips"></p>
                    	</div>
                    	<div class="pure-u-sm-15-60">
                    	    <input type="text" class="form-control input-sm" dt="mt" id="PartyDescp" name="PartyDescp" fieldname="PartyDescp" readonly />
                    	</div>
                        <div class="pure-u-sm-5-60 label-right">
                            <label for="PartyNo" class="control-label">@Resources.Locale.L_PartyDocSetup_PartyNo</label>
