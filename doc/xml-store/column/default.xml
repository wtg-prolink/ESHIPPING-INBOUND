<?xml version="1.0" encoding="UTF-8"?>
<!--
  設置需要自定義列功能的表及表中的哪些字段可以自己定義
  每個畫面上的table對應一個table結點

  column中的屬性:
  id:字段名稱
  alian:對齊方式
  width:列寬
  html:是否是html類型
  caption:列的顯示標簽
-->
<config>
  <AMS>
    <column id="CNTRY_CODE" align="left" width="70" html="N" caption="國別" />
    <column id="SHIPPER" align="left" width="150" html="N" caption="出貨人" />
    <column id="TOTAL_WEIGHT" align="right" width="80" html="N" caption="總重量" />
	  <column id="WEIGHT_UNIT" align="left" width="100" html="N" caption="重量單位" />
	  <column id="MEASURE" align="right" width="80" html="N" caption="測量數值" />
	  <column id="MEASURE_UNIT" align="left" width="100" html="N" caption="測量單位" />
	  <column id="QTY" align="left" width="80" html="N" caption="數量" />
	  <column id="IMO_NO" align="left" width="120" html="N" caption="IMO號碼" />
	  <column id="CNTRY_ORI" align="left" width="200" html="N" caption="原始港國家" />
	  <column id="ARRIVAL_PORT" align="left" width="200" html="N" caption="目的港" />
	  <column id="FINIAL_DEST" align="left" width="200" html="N" caption="最終目的地" />
    <column id="ARRIVAL_DATE" align="left" width="120" html="N" caption="抵港日期" DisplayFormat="YYYY/MM/DD HH:MM" StoreFormat="YYYYMMDDHHMM" />
	  <column id="CONT_NO" align="left" width="100" html="N" caption="貨櫃號" />
	  <column id="CONT_PIECE_COUNT" align="left" width="80" html="N" caption="容器件數" />
	  <column id="COMMODITY" align="left" width="200" html="N" caption="備註" />
	  <column id="CLEAR_DISTRICT" align="left" width="150" html="N" caption="結帳地點" />
  </AMS>
  <IPQ050>
